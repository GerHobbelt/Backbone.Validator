{
  "name": "backbone.validator",
  "description": "Validation plugin for Backbone allowing for setting of default values",
  "version": "1.1.3",
  "repository": {
    "url": "git://github.com/toddself/Backbone.Validator.git"
  },
  "main": "Backbone.Validator.js",
  "scripts": {
    "postinstall": "if [[ -d .git ]]; then cp pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit; fi",
    "pre-commit": "npm run lint && npm test",
    "prepublish": "docme README.md",
    "lint": "jshint",
    "test": "tape test/*js"
  },
  "dependencies": {
    "backbone": "~1.1.0"
  },
  "devDependencies": {
    "jshint": "~2.3.0",
    "tape": "~2.3.0",
    "docme": "~0.1.4"
  },
  "author": {
    "name": "Todd Kennedy",
    "email": "<todd@selfassembled.org>",
    "url": "http://tck.io"
  },
  "keywords": [
    "backbone",
    "models",
    "validation",
    "validator",
    "validity",
    "underscore",
    "plugin"
  ],
  "license": {
    "type": "MIT",
    "url": "https://github.com/toddself/Backbone.Validator/blog/master/LICENSE"
  },
  "engine": {
    "node": ">=0.8"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
