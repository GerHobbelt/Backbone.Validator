{
  "name": "Backbone.Validator",
  "description": "Validation plugin for Backbone allowing for setting of default values",
  "version": "0.0.0",
  "repository": {
    "url": "git://github.com/toddself/Backbone.Validator.git"
  },
  "main": "Backbone.Validator.js",
  "scripts": {
    "postinstall": "if [[ -d .git ]]; then cp pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit; fi",
    "pre-commit": "npm run lint && npm test",
    "lint": "jshint",
    "test": "tape test/*js"
  },
  "dependencies": {
    "es5-shim": "~2.1.0",
    "backbone": "~1.1.0"
  },
  "devDependencies": {
    "jshint": "~2.3.0",
    "tape": "~2.3.0"
  },
  "author": {
    "name": "Todd Kennedy",
    "email": "<todd@selfassembled.org>",
    "url": "http://tck.io"
  },
  "keywords": [],
  "license": {
    "type": "MIT",
    "url": "https://github.com/toddself/Backbone.Validator/blog/master/LICENSE"
  },
  "engine": {
    "node": ">=0.8"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
